<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration 
    PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<configuration>

	<settings>
		<setting name="defaultStatementTimeout" value="5" />
	</settings>

	<typeAliases>
		<typeAlias alias="User" type="com.duolanjian.java.market.bean.User" />
		<typeAlias alias="Order" type="com.duolanjian.java.market.bean.Order" />
		<typeAlias alias="Shop" type="com.duolanjian.java.market.bean.Shop" />
		<typeAlias alias="Commodity" type="com.duolanjian.java.market.bean.Commodity" />
		<typeAlias alias="Address" type="com.duolanjian.java.market.bean.Address" />
		<typeAlias alias="Cart" type="com.duolanjian.java.market.bean.Cart" />
		<typeAlias alias="OrderDetail" type="com.duolanjian.java.market.bean.OrderDetail" />
		
		
		<typeAlias alias="Evaluate" type="com.duolanjian.java.market.bean.Evaluate" />
		<typeAlias alias="File" type="com.duolanjian.java.market.bean.File" />
		
	</typeAliases>
	
	<mappers>
		<mapper resource="mapper/UserMapper.xml" />
		<mapper resource="mapper/OrderMapper.xml"/>
		<mapper resource="mapper/ShopMapper.xml"/>
		<mapper resource="mapper/CommodityMapper.xml"/>
		<mapper resource="mapper/AddressMapper.xml"/>
		<mapper resource="mapper/CartMapper.xml"/>
        <mapper resource="mapper/OrderDetailMapper.xml"/>
		<mapper resource="mapper/EvaluateMapper.xml"/>
		<mapper resource="mapper/FileMapper.xml"/>
		
	</mappers>

</configuration>

